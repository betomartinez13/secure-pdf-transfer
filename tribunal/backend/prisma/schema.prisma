datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Case {
  id            Int      @id @default(autoincrement())
  caseName      String
  fileName      String
  encryptedFile Bytes
  encryptedKey  String
  iv            String
  authTag       String
  hash          String
  receivedAt    DateTime @default(now())
}
